<?xml version="1.0" encoding="UTF-8"?>
<datasources xmlns="http://www.jboss.org/ironjacamar/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.jboss.org/ironjacamar/schema http://docs.jboss.org/ironjacamar/schema/datasources_1_0.xsd">
   <!-- The datasource is bound into JNDI at this location. We reference 
      this in META-INF/persistence.xml -->
    <datasource jndi-name="java:jboss/datasources/solidariedadebethaDS" pool-name="solidariedadebetha" enabled="true" use-java-context="true">
        <connection-url>jdbc:postgresql://localhost:5432/solidariedadebetha</connection-url>
        <driver>org.postgresql</driver>
        <driver-class>org.postgresql.Driver</driver-class>
        <security>
            <user-name>postgres</user-name>
            <password>admin</password>
        </security>
    </datasource>    
</datasources>
 
